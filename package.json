{
    "name": "onboard-qs",
    "version": "1.3.0",
    "type": "module",
    "description": "Qlik Sense extension for interactive app onboarding tours",
    "license": "MIT",
    "author": "GÃ¶ran Sander, Ptarmigan Labs",
    "bugs": {
        "url": "https://github.com/ptarmiganlabs/onboard.qs/issues"
    },
    "homepage": "https://github.com/ptarmiganlabs/onboard.qs#readme",
    "keywords": [
        "qlik",
        "qliksense",
        "nebula",
        "stardust",
        "onboarding",
        "tour",
        "driver.js",
        "ptarmiganlabs"
    ],
    "files": [
        "dist",
        "onboard-qs.js",
        "onboard-qs.qext"
    ],
    "engines": {
        "node": ">=18.10.0"
    },
    "main": "dist/onboard-qs.js",
    "module": "dist/onboard-qs.esm.js",
    "scripts": {
        "build": "cross-env BUILD_TYPE=development nebula build --config nebula.config.cjs --sourcemap false --mode development",
        "build:dist": "cross-env BUILD_TYPE=production nebula build --config nebula.config.cjs --sourcemap false",
        "lint": "eslint src scripts",
        "lint:fix": "eslint src scripts --fix",
        "format": "prettier --write \"./**/*.{js,mjs,css,json,yaml,md}\"",
        "format:check": "prettier --check \"./**/*.{js,mjs,css,json,yaml,md}\"",
        "start": "nebula serve",
        "sense": "nebula sense --meta src/meta.json",
        "post-build": "node scripts/post-build.mjs",
        "zip": "node scripts/zip-extension.mjs",
        "pack:dev": "cross-env BUILD_TYPE=development npm run build && npm run sense && cross-env BUILD_TYPE=development npm run post-build && npm run zip",
        "pack:prod": "cross-env BUILD_TYPE=production npm run build:dist && cross-env BUILD_TYPE=production npm run sense && cross-env BUILD_TYPE=production npm run post-build && npm run zip",
        "cloc": "cloc --exclude-dir=node_modules src/",
        "version:major": "npm version major --no-git-tag-version",
        "version:minor": "npm version minor --no-git-tag-version",
        "version:patch": "npm version patch --no-git-tag-version"
    },
    "dependencies": {
        "driver.js": "^1.3.1"
    },
    "devDependencies": {
        "@eslint/js": "^10.0.1",
        "@nebula.js/cli": "^6.5.0",
        "@nebula.js/cli-build": "^6.5.0",
        "@nebula.js/cli-sense": "^6.5.0",
        "@nebula.js/cli-serve": "^6.5.0",
        "@rollup/plugin-replace": "^6.0.2",
        "archiver": "^7.0.1",
        "cloc": "^2.6.0-cloc",
        "cross-env": "^10.1.0",
        "esbuild": "^0.27.3",
        "eslint": "^10.0.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-jsdoc": "^62.7.1",
        "globals": "^17.3.0",
        "prettier": "^3.8.1"
    },
    "peerDependencies": {
        "@nebula.js/stardust": ">=5.0.0"
    }
}
